{
	"manifest_version": 2,
	"name": "nDjLuizCampos",
	"version": "1.0",
	"description": "Troque todas as imagens do site por fotos do famoso Dj Luiz Campos.",
	"icons": {
		"16": "icon16.png",
    	"48": "icon48.png",
        "128": "icon128.png"
    },
	"content_scripts": [
	    {
	      "matches": ["*://*/*"],
	      "js": ["jquery.js", "nDjLuizCampos.js"]
	    }
  	]
}